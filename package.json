{
  "readme": "https://github.com/hisptz/action-tracker-standalone/blob/develop/README.md",
  "name": "action-tracker-standalone",
  "version": "2.0.0-alpha.00",
  "description": "The action tracker is a management tool that helps improve effective coverage of priority interventions at district and community level, covering various interventions such as maternal, newborn, and child health (MNCH), HIV, and nutrition. ",
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "build": "d2-app-scripts build --no-verify",
    "start": "dotenv -- cross-var d2-app-scripts start",
    "test": "d2-app-scripts test",
    "e2e": "concurrently 'yarn cy:start' 'wait-on 'http-get://localhost:3000' && cypress run' --kill-others --success first",
    "e2e:interactive": "npx cypress open",
    "deploy": "d2-app-scripts deploy",
    "cy:start": "BROWSER=none yarn start",
    "cy:run": "wait-on 'http-get://localhost:3000' && cypress run",
    "cy:capture": "concurrently 'yarn cy:start' 'yarn cy:run --env networkMode=capture ' --kill-others --success first",
    "cy:stub": "concurrently 'yarn cy:start' 'yarn cy:run --env networkMode=stub' --kill-others --success first",
    "cy:open": "concurrently 'BROWSER=none yarn start' 'wait-on 'http-get://localhost:3000' && cypress open' --kill-others --success first ",
    "cy:open:capture": "concurrently 'BROWSER=none yarn start' 'wait-on 'http-get://localhost:3000' && cypress open --env networkMode=capture' --kill-others --success first ",
    "cy:open:stub": "concurrently 'BROWSER=none yarn start' 'wait-on 'http-get://localhost:3000' && cypress open --env networkMode=stub' --kill-others --success first "
  },
  "author": {
    "email": "info@hisptanzania.org",
    "name": "HISP Tanzania",
    "url": "https://hisptanzania.org/"
  },
  "devDependencies": {
    "@cypress/react": "^7.0.3",
    "@dhis2/cli-app-scripts": "^10.3.9",
    "@dhis2/cli-utils-cypress": "^10.0.3",
    "@dhis2/cypress-commands": "^10.0.3",
    "@dhis2/cypress-plugins": "^10.0.3",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.196",
    "@types/luxon": "^3.3.1",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "concurrently": "^8.2.0",
    "cross-var": "^1.1.0",
    "cucumber": "^7.0.0-rc.0",
    "cypress": "^12.17.2",
    "cypress-multi-reporters": "^1.6.3",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.45.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.0",
    "typescript": "5.1.6",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "@dhis2/app-runtime": "^3.9.4",
    "@dhis2/app-service-datastore": "^1.0.0-alpha.2",
    "@dhis2/ui": "8.13.14",
    "@hisptz/dhis2-ui": "^1.0.51",
    "@hisptz/dhis2-utils": "^1.0.51",
    "@tanstack/react-table": "^8.9.3",
    "async": "^3.2.2",
    "async-es": "^3.2.4",
    "classnames": "^2.3.2",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "react-color": "^2.19.3",
    "react-hook-form": "^7.45.2",
    "react-router-dom": "^6.14.2",
    "react-to-print": "^2.14.13",
    "recoil": "^0.7.7",
    "usehooks-ts": "^2.9.1",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "xlsx": "^0.18.5",
    "zod": "^3.21.4"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": [
      "cypress/e2e/[filepath]/**/*.{js,ts}",
      "cypress/e2e/[filepath].{js,ts}",
      "cypress/e2e/common/**/*.{js,ts}"
    ]
  }
}
